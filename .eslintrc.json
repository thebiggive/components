{
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": [
    "plugin:@stencil-community/recommended",
    "plugin:prettier/recommended",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "@stencil-community/decorators-style": "off",
    "@stencil-community/required-jsdoc": "off"
  }
}
